	{{parent()}}
	<script type="text/javascript">
		$('.je-minscris').click( function(e){ 
		    var speed = 1200; 
		    $elt = $(this).data('id');
		    $('html, body').animate({scrollTop:($("#"+$elt).offset().top)-200}, speed);
		});
	   	function validateContactForm(){
		    try{
		        var validator = $('#formInscription').validate({
		            rules:{
		                prenom:{
		                    required:true
		                },
		                email:{
		                    required:true,
		                    email:true
		                }, 
		                phone:{
		                    required:true
		                },   
		                CGU:{
		                    required:true
		                }             
		            }
		        });
		        
		        var $validated = $('#formInscription').valid();
		        return $validated;
		    }catch(error){
		        console.log(error)
		    }
		}

		var form = document.forms.namedItem("formInscription");
		form.addEventListener('submit', function(ev) {
			if( !validateContactForm()){
				toastr.error("Veuillez renseigner toutes les informations.");
		    	$('#formInscription button[type=submit]').text("Je m'inscris !");
		        ev.preventDefault();
		        return false;
		    }

			$('#formInscription button[type=submit]').text('Inscription...');
		    $('.lds-ripple').css('display','inline-block');
		    $('#formInscription button[type=submit]').css('pointer-events','none');
			$.ajax({
		        url:"{{ path('fos_user_registration_register_xhr') }}",
		        type:"POST",
		        data: new FormData(this),
	            dataType: 'json',
	            contentType: false,
	            cache: false,
	            processData:false,
		        //data:$('#formTournoi').serialize(),
		        success:function(response) {
		        	toastr.success(response.responseJSON);
			    	$('#formInscription button[type=submit]').button('reset');
			    	$('#formInscription button[type=submit]').text("Je m'inscris !");
			    	$('.lds-ripple').css('display','none');
			    	$('#formInscription button[type=submit]').css('pointer-events','auto');
			    	$(".success-checkmark").show().delay(3000).fadeOut();
	                $(".check-icon").show().show().delay(3000).fadeOut();
			    	window.location.href = "{{path('inscription_home')}}";

		        },
		        error:function(response){
		        	toastr.error(response.responseJSON);
			    	$('#formInscription button[type=submit]').button('reset');
			    	$('#formInscription button[type=submit]').text("Je m'inscris !");
			    	$('.lds-ripple').css('display','none');
			    	$('#formInscription button[type=submit]').css('pointer-events','auto');
			    }
		   	});
				
			ev.preventDefault();
		}, false);


	</script>
