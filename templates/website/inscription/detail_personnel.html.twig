<div class="wiz-header">
	<h2 class="title">INFORMATIONS PERSONNELLES</h2>
	<hr class="underline-title">
</div>
<div class="wiz-top-desc"></div>
<form class="card card-body" method="POST" id="infoPersonnelle" id="infoPersonnelle" data-logged="{{ app.user is not null ? 'logged' : '' }}">
    <input type="hidden" name="password" value="">
    <div class="form-row">
      <div class="col-sm-6">
        <div class="form-group">
          <label for="planner-name">Prénom</label>
          <input type="text" name="prenom" class="form-control" id="prenom" value="{{informations is not null ? informations.prenom : '' }}">
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label for="planner-email">Deuxième nom</label>
          <input type="text" name="second_name" class="form-control" id="second_name" value="{{informations is not null ? informations.getSecondName }}">
        </div>
      </div>
    </div>
    <div class="form-row">
      <div class="col-sm-6">
        <div class="form-group">
          <label for="planner-email">Nom de famille</label>
          <input type="text" name="nom_famille" class="form-control" id="nom_famille" value="{{informations is not null ? informations.getNomFamille }}">
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label for="planner-industry">Nationalité</label>
          <div class="position-relative">
            <select name="nationalite" class="custom-select" id="nationalite">
              {% for c in countries %}
                <option value="{{c.code}}-{{c.name}}" {% if (informations is not null) and (informations.nationalite == c.code~'-'~c.name) %} selected="selected" {% endif %}>{{c.name}}</option>
              {% endfor %}
            </select>
          </div>
        </div>
      </div>
    </div>
    <div class="form-row">
      <div class="col-sm-6">
        <div class="form-group">
          <label for="email">E-mail</label>
          <input type="email" name="email" class="form-control" value="{{informations is not null ? informations.email : ''}}" id="email" placeholder="">
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label for="telephone">Numéro de téléphone</label>
          <input type="text" name="telephone" class="form-control" id="telephone" placeholder="Ex:0033548895" value="{{informations is not null ? informations.telephone : ''}}">
        </div>
      </div>
    </div>
    <div class="form-row">
        <div class="col-sm-6">
            <div class="form-group">
              <label for="jour_naiss">Date de naissance</label>
              <div class="position-relative list-elt-birth">
                <div class="elt-birth">
                	<select name="jour_naiss" class="custom-select" id="jour_naiss">
                		<option value="" disabled>Jour</option>
                		{% for i in 1..31 %}
                		    <option value="{{i}}">{{i}}</option>
                		{% endfor %}
                    </select>
                </div>
                <div class="elt-birth" style="flex: 2">
                	<select name="mois_naiss" class="custom-select" id="mois_naiss">
                		{% for m in mois %}
                			<option value="{{m.number}}">{{m.val}}</option>
                		{% endfor %}
                    </select>
                </div>
                <div class="elt-birth">
                	<select name="annee_naiss" class="custom-select" id="annee_naiss">
                		<option value="" disabled>Année</option>
                		{% for i in 2002..1987 %}
                		    <option value="{{i}}">{{i}}</option>
                		{% endfor %}
                    </select>
                </div>
              </div>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
              <label for="lieu_naiss">Lieu de naissance</label>
              <input type="text" name="lieu_naiss" class="form-control" id="lieu_naiss" placeholder="" value="{{informations is not null ? informations.getLieuNaiss}}">
            </div>
        </div>
    </div>
    <div class="form-row">
      <div class="col-sm-6">
        <div class="form-group">
          <label for="ident_type">Type d'identification</label>
          <select name="ident_type" class="custom-select" id="ident_type">
              <option value="cni" {% if (informations is not null) and (informations.getIdentificationType == 'cni') %} selected="selected" {% endif %}>Carte Nationale ID</option>
              <option value="passport" {% if (informations is not null) and (informations.getIdentificationType == 'passport') %} selected="selected" {% endif %}>Passport</option>
          </select>
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label for="planner-email">Numéro d'identité</label>
          <input type="text" name="ident_number" class="form-control" id="ident_number" value="{{informations is not null ? informations.getNumeroIdentite}}">
        </div>
      </div>
    </div>
    <div class="form-row">
    	<div class="col-sm-12 d-flex flex-column flex-wrap flex-md-row justify-content-between align-items-center">
            <div class="custom-control custom-checkbox mb-3 mb-md-0">
                <input type="checkbox" class="custom-control-input" id="planner-agree" name="referred">
                <label class="custom-control-label" for="planner-agree"><small>Avez-vous été référé par quelqu'un ? (optionnel)</small>
                </label>
            </div>
        </div>
    </div>

    <div class="wiz-header" style="margin-top: 4rem">
	<h2 class="title">ADRESSE RÉSIDENTIELLE PRINCIPALE</h2>
	<hr class="underline-title">
	</div>
	<div class="wiz-top-desc" style="margin-top: 0"></div>
	<div class="form-row">
      <div class="col-sm-6">
        <div class="form-group">
          <label for="pays_residence">Pays de résidence principale</label>
          <select name="pays_residence" class="custom-select" id="pays_residence">
              {% for c in countries %}
                <option value="{{c.code}}-{{c.name}}">{{c.name}}</option>
              {% endfor %}
          </select>
        </div>
      </div>
    </div>
    <div class="form-row">
      <div class="col-sm-6">
        <div class="form-group">
          <label for="adresse">Adresse</label>
          <input type="text" name="adresse" class="form-control" id="adresse" value="{{informations is not null ? informations.adresse}}">
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label for="province">Province / Etat</label>
          <input type="text" name="province" class="form-control" id="province" value="{{informations is not null ? informations.etat}}">
        </div>
      </div>
    </div>
    <div class="form-row">
      <div class="col-sm-6">
        <div class="form-group">
          <label for="ville">Ville</label>
          <input type="text" name="ville" class="form-control" id="ville" value="{{informations is not null ? informations.ville}}">
        </div>
      </div>
      <div class="col-sm-6">
        <div class="form-group">
          <label for="codepostal">Code postal</label>
          <input type="text" name="codepostal" class="form-control" id="codepostal" value="{{informations is not null ? informations.getCodePostale}}">
        </div>
      </div>
    </div>
</form>