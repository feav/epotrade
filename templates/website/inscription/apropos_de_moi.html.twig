				<label for="depot_4" class="item-elt label-form">101 000 $ - 250 000 $</label>
				<input id="depot_5" type="radio" name="depot" value="251 000 $ - 500 000 $">
				<label for="depot_5" class="item-elt label-form">251 000 $ - 500 000 $</label>
				<input id="depot_6" type="radio" name="depot" value="501 K $ - 1 M $">
